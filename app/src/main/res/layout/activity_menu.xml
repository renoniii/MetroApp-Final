<?xml version="1.0" encoding="utf-8"?>
 <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu"
    android:background="@drawable/inicio">


       <ImageView
           android:id="@+id/img_rutas"
           android:layout_width="110dp"
           android:layout_height="130dp"
           android:layout_marginTop="130dp"
           android:layout_marginBottom="16dp"
           android:onClick="onClickRutas"
           app:layout_constraintBottom_toTopOf="@+id/imageView4"
           app:layout_constraintEnd_toEndOf="parent"
           app:layout_constraintHorizontal_bias="0.498"
           app:layout_constraintStart_toStartOf="parent"
           app:layout_constraintTop_toTopOf="parent"
           app:srcCompat="@drawable/icono_rutas"
           tools:ignore="SpeakableTextPresentCheck" />

       <ImageView
           android:id="@+id/imageView5"
           android:layout_width="110dp"
           android:layout_height="130dp"
           android:layout_marginTop="16dp"
           android:layout_marginBottom="170dp"
           android:onClick="onClickEstaciones"
           app:layout_constraintBottom_toBottomOf="parent"
           app:layout_constraintEnd_toEndOf="parent"
           app:layout_constraintHorizontal_bias="0.498"
           app:layout_constraintStart_toStartOf="parent"
           app:layout_constraintTop_toBottomOf="@+id/imageView4"
           app:srcCompat="@drawable/icono_recarga"
           tools:ignore="SpeakableTextPresentCheck" />

      <TextView
          android:id="@+id/textView7"
          android:layout_width="125dp"
          android:layout_height="64dp"
          android:layout_marginTop="-12dp"
          android:text="Estacion de recarga"
          android:textAlignment="center"
          android:textColor="@color/green"
          android:textSize="20sp"
          android:textStyle="bold"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/imageView5" />

      <ImageView
          android:id="@+id/imageView4"
          android:layout_width="110dp"
          android:layout_height="130dp"
          android:layout_marginTop="16dp"
          android:layout_marginBottom="16dp"
          app:layout_constraintBottom_toTopOf="@+id/imageView5"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintHorizontal_bias="0.498"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/img_rutas"
          app:srcCompat="@drawable/icono_tarjeta" />

      <TextView
          android:id="@+id/textView6"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginTop="-12dp"
          android:text="Tarjeta"
          android:textColor="@color/green"
          android:textSize="20sp"
          android:textStyle="bold"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/imageView4" />

      <TextView
          android:id="@+id/textView5"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginTop="-12dp"
          android:text="Rutas"
          android:textColor="@color/green"
          android:textSize="20sp"
          android:textStyle="bold"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintHorizontal_bias="0.498"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/img_rutas" />

       <ImageView
          android:id="@+id/btnUser"
          android:layout_width="63dp"
          android:layout_height="64dp"
          android:layout_marginStart="12dp"
          android:layout_marginTop="44dp"
          android:layout_marginEnd="316dp"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintHorizontal_bias="0.7"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent"
          app:srcCompat="@drawable/icono_perfil" />



</androidx.constraintlayout.widget.ConstraintLayout>